rm -rf ./my-t3-app/
rm -rf .DS_Store
rm -rf .env-example
rm -rf .env
rm -rf .eslintrc.json
rm -rf .gitignore
rm -rf .npmrc
rm -rf .yarnrc
rm -rf README.md
rm -rf next-env.d.ts
rm -rf next.config.js
rm -rf next.config.mjs
rm -rf node_modules
rm -rf package-lock.json
rm -rf package.json
rm -rf postcss.config.cjs
rm -rf postcss.config.js
rm -rf prettier.config.cjs
rm -rf prisma
rm -rf public
rm -rf src
rm -rf tailwind.config.cjs
rm -rf tailwind.config.js
rm -rf tsconfig.json

yarn create t3-app --noGit --noInstall --tailwind true --nextAuth true --prisma true --trpc true --default
	
find my-t3-app -maxdepth 1 -not -name my-t3-app -exec sh -c 'mv "$@" "$0"' ./ {} +
	
yarn install
yarn prisma db push

git add .
git commit -m "chore: run update script"
